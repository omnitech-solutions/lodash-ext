{
  "name": "@omnitech/lodash-ext",
  "version": "1.0.4",
  "description": "A library template writing by ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:x-cold/rollup-ts-library-boilerplate.git"
  },
  "scripts": {
    "prepare": "rm -rf .husky/_ && husky install",
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "eslint --ext .js,.ts --fix src/",
    "start": "rollup -c rollup.config.js -w",
    "docs": "typedoc --out docs --theme minimal src",
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "release": "npm run release:patch",
    "release:patch": "npm run build && standard-version --release-as patch",
    "release:minor": "npm run build && standard-version --release-as minor",
    "release:major": "npm run build && standard-version --release-as major",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy": "rimraf docs/ && npm run docs"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "keywords": [
    "rollup",
    "typescript",
    "library",
    "boilerplate"
  ],
  "author": "x-cold <lxstart@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "rimraf": "^4.3.1",
    "rollup": "^3.18.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.26"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
